#! /usr/bin/env ruby
def main()
	sig = 0;
	shell = File.basename(ENV['SHELL']);
	if shell=='sh'||shell=='bash'
		puts "Note, using shell: #{shell}";
	elsif shell=='csh'
		puts "Note, using shell: #{shell}";
	else
		puts "Error, unsupported shell type: #{shell}";
		return 1;
	end

	return sig;
end



SIG = main();
exit SIG;

